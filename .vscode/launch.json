  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run with MongoDB input and SQL output",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "args": ["-input-type", "mongodb", "-o", "example-output.sql"],
      "program": "${workspaceRoot}/main.go",
      "env": { "ENV": "dev" },
      "showLog": true
    }
  ]
}
